							<psfp:index>1</psfp:index>
							<psfp:operation-name>psfp:set-gate-and-ipv</psfp:operation-name>
							<psfp:gate-state-value>closed</psfp:gate-state-value>
							<psfp:ipv-spec>three</psfp:ipv-spec>
							<psfp:time-interval-value>8000</psfp:time-interval-value>
						</psfp:gate-control-entry>
					</psfp:admin-control-list>
					<psfp:config-change>true</psfp:config-change>
				</sfsg:stream-gate-instance-table>
				<psfp:supported-list-max>32</psfp:supported-list-max>
				<psfp:supported-cycle-max>
					<psfp:numerator>1</psfp:numerator>
					<psfp:denominator>1</psfp:denominator>
				</psfp:supported-cycle-max>
