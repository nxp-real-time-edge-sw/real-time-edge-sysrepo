<bridges xmlns="urn:ieee:std:802.1Q:yang:ieee802-dot1q-bridge"
         xmlns:sfsg="urn:ieee:std:802.1Q:yang:ieee802-dot1q-stream-filters-gates"
         xmlns:psfp="urn:ieee:std:802.1Q:yang:ieee802-dot1q-psfp"
	 xmlns:qci-augment="urn:ieee:std:802.1Q:yang:ieee802-dot1q-qci-augment">
	<bridge>
		<name>switch</name>
		<bridge-type>provider-edge-bridge</bridge-type>
		<address>00-00-00-00-00-02</address>
		<component>
		    <name>swp0</name>
		    <type>edge-relay-component</type>
		    <sfsg:stream-filters>
			<sfsg:stream-filter-instance-table>
			    <sfsg:stream-filter-instance-id>1</sfsg:stream-filter-instance-id>
			    <sfsg:stream-handle>1</sfsg:stream-handle>
			    <qci-augment:stream-filter-enabled>true</qci-augment:stream-filter-enabled>
			    <sfsg:priority-spec>four</sfsg:priority-spec>
			    <sfsg:stream-gate-ref>1</sfsg:stream-gate-ref>
			    <psfp:flow-meter-instance-id>68</psfp:flow-meter-instance-id>
			    <sfsg:max-sdu-size>800</sfsg:max-sdu-size>
			    <sfsg:stream-blocked-due-to-oversize-frame-enabled>false</sfsg:stream-blocked-due-to-oversize-frame-enabled>
		    </sfsg:stream-filter-instance-table>
	    </sfsg:stream-filters>
            <sfsg:stream-gates>
                <sfsg:stream-gate-instance-table>
                    <sfsg:stream-gate-instance-id>1</sfsg:stream-gate-instance-id>
                    <sfsg:gate-enable>true</sfsg:gate-enable>
                    <sfsg:admin-gate-states>open</sfsg:admin-gate-states>
                    <sfsg:admin-ipv>null</sfsg:admin-ipv>
		    <psfp:admin-base-time>
			    <psfp:seconds>0</psfp:seconds>
			    <psfp:nanoseconds>0</psfp:nanoseconds>
		    </psfp:admin-base-time>
		    <psfp:admin-cycle-time>
			    <psfp:numerator>8000</psfp:numerator>
			    <psfp:denominator>1000000000</psfp:denominator>
		    </psfp:admin-cycle-time>
		    <psfp:admin-control-list>
			<psfp:gate-control-entry>
                            <psfp:index>0</psfp:index>
                            <psfp:operation-name>psfp:set-gate-and-ipv</psfp:operation-name>
                            <psfp:gate-state-value>open</psfp:gate-state-value>
                            <psfp:ipv-spec>two</psfp:ipv-spec>
                            <psfp:time-interval-value>8000</psfp:time-interval-value>
                        </psfp:gate-control-entry>
                    </psfp:admin-control-list>
                </sfsg:stream-gate-instance-table>
		<psfp:supported-list-max>32</psfp:supported-list-max>
				<psfp:supported-cycle-max>
					<psfp:numerator>1</psfp:numerator>
					<psfp:denominator>1</psfp:denominator>
				</psfp:supported-cycle-max>
				<psfp:supported-interval-max>1000000000</psfp:supported-interval-max>
	</sfsg:stream-gates>
		</component>
	</bridge>
</bridges>
